# Database
DATABASE_URL="postgresql://user:password@localhost:5432/agentflow?schema=public"

# Authentication
AUTH_SECRET="your-secret-key-here"
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret"

# OAuth Providers (optional)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
GITHUB_CLIENT_ID=""
GITHUB_CLIENT_SECRET=""

# AI Providers
OPENAI_API_KEY=""
ANTHROPIC_API_KEY=""
GOOGLE_AI_API_KEY=""

# Google Custom Search Engine (for web search tool)
GOOGLE_CSE_API_KEY=""
GOOGLE_CSE_CX=""

# AI Configuration
DEFAULT_LLM_PROVIDER="gemini"  # gemini, openai, anthropic, openrouter
DEFAULT_LLM_MODEL="gemini-pro"
MAX_TOKENS=2048
TEMPERATURE=0.7

# Agent Configuration
ENABLE_AGENT_LOGGING="true"
AGENT_LOG_LEVEL="info"  # error, warn, info, debug, trace

# Vector Database (Qdrant)
QDRANT_URL="http://localhost:6333"
QDRANT_API_KEY=""

# Redis (for caching and rate limiting)
REDIS_URL="redis://localhost:6379"

# Storage (S3 or compatible)
STORAGE_ACCESS_KEY_ID=""
STORAGE_SECRET_ACCESS_KEY=""
STORAGE_REGION=""
STORAGE_BUCKET=""
STORAGE_ENDPOINT=""

# Email (SMTP)
SMTP_HOST=""
SMTP_PORT=587
SMTP_USER=""
SMTP_PASSWORD=""
SMTP_FROM="AgentFlow <noreply@agentflow.pro>"

# Monitoring
SENTRY_DSN=""

# Feature Flags
ENABLE_SIGNUP="true"
REQUIRE_EMAIL_VERIFICATION="false"

# Environment
NODE_ENV="development"
